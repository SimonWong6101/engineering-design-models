!
! SDRC I-DEAS color defaults file
! This file contains user interface colors:
!


! Default foreground and background color
*foreground:        #FF00E8009400
*background:        #2A0047005E00


! Default top and bottom shadow colors
*topShadowColor:    #62A97D068DF9
*bottomShadowColor: #08000C001C00


! Default Highlight color (execute arm color)
*armColor:          #4848D1D1CCCC


! Default Select Color (toggle and radio ON color)
*selectColor:       #4848D1D1CCCC


! Default Border Color (startup project area)
*borderColor:       #08000C001C00


!
! Vector Graphics Colors (explicitly set by GipInitParam)
!


! primary
!
vgr.yellow:	#FFFFD7D70000
!
! secondary
!
vgr.green:	#0000CDCD0000
!
! annotation
!
vgr.cyan:       #4848D1D1CCCC
!
! warning
!
vgr.red:	#CDCD00000000
!
! pull down menu arrow
!
vgr.blue:       #4848D1D1CCCC
!
! others
!		
vgr.magenta:	#4848D1D1CCCC


! I-DEAS Test Real Time Display
!I-DEAS Real Time*background:	#2A0047005E00
!I-DEAS Real Time*foreground:	#FF00E8009400
!I-DEAS Real Time*highlight:	#FF00E8009400
!I-DEAS Real Time*borderColor:	#FF00E8009400


!
! Smart View colors
!


! Search hit color (sort of a highlight)
SMART_VIEW*shColor:        #CDCD00000000


! Hyper text link color. This also has a box around it.
SMART_VIEW*hlColor:        #2A0047005E00


! General text color
SMART_VIEW*txColor:        #08000C001C00
SMART_VIEW*whitePixel:     #FF00E8009400
SMART_VIEW*blackPixel:     #08000C001C00


! Color for text in search list
SMART_VIEW*mlSelectionTopLevelShell*XmToggleButton*foreground:  #FF00E8009400
SMART_VIEW*mlSelectionTopLevelShell*background:  #2A0047005E00


! Color for text area background
SMART_VIEW*XmDrawingArea*background: #FF00E8009400
SMART_VIEW*XmDrawingArea*foreground: #08000C001C00


! buttons, menus etc. 
SMART_VIEW*foreground: #FF00E8009400
SMART_VIEW*background: #2A0047005E00
SMART_VIEW*XmPushButton*topShadowColor:    #62A97D0c8DF9
SMART_VIEW*XmPushButton*bottomShadowColor: #08000C001C00
SMART_VIEW*XmArrowButton*topShadowColor:    #62A97D0c8DF9
SMART_VIEW*XmArrowButton*bottomShadowColor: #08000C001C00
SMART_VIEW*XmScrollBar*topShadowColor:    #62A97D0c8DF9
SMART_VIEW*XmScrollBar*bottomShadowColor: #08000C001C00
SMART_VIEW*XmSeparator*topShadowColor:    #62A97D0c8DF9
SMART_VIEW*XmSeparator*bottomShadowColor: #08000C001C00
SMART_VIEW*XmFrame*topShadowColor:    #62A97D0c8DF9
SMART_VIEW*XmFrame*bottomShadowColor: #08000C001C00
SMART_VIEW*XmCascadeButton*topShadowColor:    #62A97D0c8DF9
SMART_VIEW*XmCascadeButton*bottomShadowColor: #08000C001C00
SMART_VIEW*XmPulldownMenu*topShadowColor:    #62A97D0c8DF9
SMART_VIEW*XmPulldownMenu*bottomShadowColor: #08000C001C00
SMART_VIEW*XmPopupMenu*topShadowColor:    #62A97D0c8DF9
SMART_VIEW*XmPopupMenu*bottomShadowColor: #08000C001C00
SMART_VIEW*XmRowColumn*topShadowColor:    #62A97D0c8DF9
SMART_VIEW*XmRowColumn*bottomShadowColor: #08000C001C00
!
