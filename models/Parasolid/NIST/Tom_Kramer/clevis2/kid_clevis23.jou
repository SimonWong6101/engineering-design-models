(modeller start)
(load "plisp/load_functions.l")
(define workpiece body)
(workpiece receive "olps_workpiece_in.xmt_txt")
(workpiece point '(0 0 0))
(workpiece direction '(1.0 0.0 0.0))
(workpiece angle 90.0)
(workpiece rotate)
(translate_body -0.1 1.49 0.0)
(init_graphics)
(make_pocket 'rectangular_pocket1 1.75 1.6 0.61 4.8 1.0 2.39 0.5)
(workpiece subtract 'rectangular_pocket1)
(graphics clear)
(graphics sketch 'workpiece)
(make_pocket 'rectangular_pocket2 4.95 1.5 0.61 4.0 1.0 2.39 0.5)
(workpiece subtract 'rectangular_pocket2)
(graphics clear)
(graphics sketch 'workpiece)
(make_pocket 'rectangular_pocket3 1.75 1.59 0.6 4.8 1.0 1.9 0.5)
(workpiece subtract 'rectangular_pocket3)
(graphics clear)
(graphics sketch 'workpiece)
(make_pocket 'rectangular_pocket4 4.95 1.49 0.6 4.0 1.0 1.5 0.5)
(workpiece subtract 'rectangular_pocket4)
(graphics clear)
(graphics sketch 'workpiece)
(workpiece merge)
(translate_body 0.1 -1.49 0.0)
(workpiece point '(0 0 0))
(workpiece direction '(1.0 0.0 0.0))
(workpiece angle -90.0)
(workpiece rotate)
(workpiece transmit "olps_workpiece_out")
(graphics clear)
(graphics sketch 'workpiece)
(graphics ar)
-- (quit)
