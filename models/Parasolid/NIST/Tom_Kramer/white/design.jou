-- This journal creates a vertical cylinder, adds a conical tip at the
-- bottom, cuts off the sides of the top by subtracting two blocks, and
-- puts a toroidal groove and a spherical dimple in the top.

(modeller start)
(load "plisp/load_functions.l")
(define workpiece p_block)
(workpiece x 6.5; y 3.0; z 1.5)
(workpiece point '(3.25 1.5 0.0))
(workpiece create)
(init_graphics)

(make_pocket 'pocket1 4.0 1.5 0.9 3.0 2.0 0.6 0.7)
(graphics sketch 'pocket1)
(workpiece subtract 'pocket1)
(graphics clear)
(graphics sketch 'workpiece)

(make_hole 'hole1 0.8 0.9 0.0 1.5 0.25)
(graphics sketch 'hole1)
(workpiece subtract 'hole1)
(graphics clear)
(graphics sketch 'workpiece)

-- (workpiece transmit "white_para")
-- (quit)
